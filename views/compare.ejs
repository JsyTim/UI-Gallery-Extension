<!-- Page Content
 -->
<div class="container-fluid">
    <h2 class="my-4 text-center text-lg-left">Widget Compare</h2>
    <hr/>
    <div>
        <span>Compare widgets with the following properties:</span><br>
        <span style="font-size: 14px; font-style: italic;">(note: do not choose the same developer)</span>
        <form name="compare" method="get" id="myform" action="/compare" class="panel-body">
            <div class="row">
                <div class="form-group form-inline">
                    <fieldset>
                        <label class="badge">Text Filter</label>
                        <input type="text" class="form-control" id="text" name="text" value="<%= query.text %>"
                               placeholder="Text Filter">
                    </fieldset>
                
                    <fieldset>
                        <label class="badge">Widget Class</label>
                        <select class="custom-select form-control" id="btnType" name="btnType">
                            <% if (!btnTypeArr.includes(query.btnType)) { %>
                            <option selected value="All">All</option>
                            <% } else { %>
                            <option value="All">All</option>
                            <% }
                            for (i = 0; i < btnTypeArr.length; i++) {
                                if (btnTypeArr[i] === 'All') {
                                    continue;
                                }
                            if (query.btnType === btnTypeArr[i]) { %>
                            <option selected> <%= btnTypeArr[i] %></option>
                            <% }else { %>
                            <option> <%= btnTypeArr[i] %></option>
                            <% }
                            } %>
                        </select>
                    </fieldset>
                    <fieldset>
                        <label class="badge">Color</label>
                        <select class="custom-select form-control" id="color" name="color">
                            <% if (!colArr.includes(query.color)) { %>
                            <option selected value="All">All</option>
                            <% } else { %>
                            <option value="All">All</option>
                            <% }
                            for (var i = 0; i < colArr.length; i++) {
                                if (colArr[i] === 'All') {
                                    continue;
                                }
                            if (query.color === colArr[i]) { %>
                            <option selected> <%= colArr[i] %></option>
                            <% }else { %>
                            <option> <%= colArr[i] %></option>
                            <% }
                            } %>
                        </select>
                    </fieldset>
                    <fieldset>
                        <label class="badge">Category</label>
                        <select class="custom-select form-control" id="category" name="category">
                            <% if (!catArr.includes(query.category)) { %>
                            <option selected value="All">All</option>
                            <% } else { %>
                            <option value="All">All</option>
                            <% }
                            for (var i = 0; i < catArr.length; i++) {
                                if (catArr[i] === 'All') {
                                    continue;
                                }
                            if (query.category === catArr[i]) { %>
                            <option selected> <%= catArr[i] %></option>
                            <% }else { %>
                            <option> <%= catArr[i] %></option>
                            <% }
                            } %>
                        </select>
                    </fieldset>
                    <fieldset>
                        <label class="badge">DeveloperOne</label>
                        <select class="custom-select form-control" id="DeveloperOne" name="DeveloperOneName" >
                            <option value="Empty"></option>
                            <option value="AI_Factory_Limited">AI_Factory_Limited</option>
                            <option value="GIOSIS_PTE._LTD.">GIOSIS_PTE._LTD.</option>
                            <option value="JRJ_Unlimited">JRJ_Unlimited</optgroup>
                            <option value="SMSROBOT_LTD">SMSROBOT_LTD</option>
                            <option value="NAVER_Corp.">NAVER_Corp.</option>
                            <option value="Corcanoe">Corcanoe</optiton>
                            <option value="Gemego_Ltd">Gemego_Ltd</optiton>
                            <option value="SMSROBOT_LTD">SMSROBOT_LTD</optiton>
                            <option value="Mirco_Bettelini">Mirco_Bettelini</optiton>
                            <option value="Boreumdal_Lab">Boreumdal_Lab</optiton>
                        </select>
                    </fieldset>
                     <fieldset>
                        <label class="badge">DeveloperTwo</label>
                        <select class="custom-select form-control" id="DeveloperTwo" name="DeveloperTwoName" >
                            <option value="Empty"></option>
                            <option value="AI_Factory_Limited">AI_Factory_Limited</option>
                            <option value="GIOSIS_PTE._LTD.">GIOSIS_PTE._LTD.</option>
                            <option value="JRJ_Unlimited">JRJ_Unlimited</optgroup>
                            <option value="SMSROBOT_LTD">SMSROBOT_LTD</option>
                            <option value="NAVER_Corp.">NAVER_Corp.</option>
                            <option value="Corcanoe">Corcanoe</optiton>
                            <option value="Gemego_Ltd">Gemego_Ltd</optiton>
                            <option value="SMSROBOT_LTD">SMSROBOT_LTD</optiton>
                            <option value="Mirco_Bettelini">Mirco_Bettelini</optiton>
                            <option value="Boreumdal_Lab">Boreumdal_Lab</optiton>
                        </select>
                    </fieldset>

                    <fieldset>
                        <label class="badge">Sort By</label>
                        <select class="custom-select form-control" id="sortType" name="sortType">
                            <% if (!((query.sortType) in sortTypeDict)) { %>
                            <option selected value="appDownloads"> <%= sortTypeDict['appDownloads'] %></option>
                            <% } else { %>
                            <option value="appDownloads"> <%= sortTypeDict['appDownloads'] %></option>
                            <% } for (const [key, value] of Object.entries(sortTypeDict)) {
                                if (key === 'appDownloads') {
                                    continue;
                                }

                            if (query.sortType === key) { %>
                            <option selected value="<%= key %>"><%= value %></option>
                            <% }else { %>
                            <option value="<%= key %>"><%= value %></option>
                            <% }
                            } %>
                        </select>
                    </fieldset>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-xs-6">
