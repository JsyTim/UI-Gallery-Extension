<script src="/javascripts/imageCell.js"></script>

<script>
    function getWidthWithHeight(){
        let widthArr;
        let heightArr;

        if (getUrlParameter('width') && getUrlParameter('height')) {
            widthArr = getUrlParameter('width').split(';');
            heightArr = getUrlParameter('height').split(';');
        } else {
            heightArr = [0, 1280];
            widthArr = [0, 800];
        }

        $("#height-slider").ionRangeSlider({
            type: "double",
            min: 0,
            max: 1280,
            from: heightArr[0],
            to: heightArr[1]
        });
        
        $("#width-slider").ionRangeSlider({
            type: "double",
            min: 0,
            max: 800,
            from: widthArr[0],
            to: widthArr[1]
        });
        let htmlCmp1 = "";
        let htmlCmp2 = "";
        let sPageURL = decodeURIComponent(window.location.search.substring(1));
        sURLVariables = sPageURL.split('&');
        if(sURLVariables.length > 4){
            var devOptionOne = sURLVariables[4].split('=')[1].toString();
            var devOptionTwo = sURLVariables[5].split('=')[1].toString();
            if(devOptionOne != devOptionTwo){
                htmlCmp1 += '</span>';
                htmlCmp1 += '<span style="font-size: 18px; font-weight: bold;">'+devOptionOne+'</span>';
                htmlCmp2 += '<span style="font-size: 18px; font-weight: bold;">'+devOptionTwo+'</span>';
                $('#DeveloperOneName').append(htmlCmp1);
                $('#DeveloperTwoName').append(htmlCmp2);
            }
        }
    }
</script>
<script>
    $(window).onload = getWidthWithHeight();
</script>
<script>
    $(document).ready(function () {
        let _mainPage;
        
        if (getUrlParameter('page')) {
            _mainPage = parseInt(getUrlParameter('page'));
            console.log("cmpload");
            loadCmpImages(_mainPage);
            console.log('loading images...');
            let timer;
            
            $(window).scroll(function () {
                if (_mainPage) {
                    if ($(window).scrollTop() >= ($(document).height() - $(window).height()) * 0.75) {
                        if (timer) {
                            window.clearTimeout(timer);
                        }
                        document.body.scrollTop = document.body.scrollHeight;
                        timer = window.setTimeout(function () {
                            _mainPage += 1;
                            loadCmpImages(_mainPage);
                        }, 500); //500
                    }
                }
            });
            
        } else {
            _mainPage = 1;
        }

    });
</script>
</body>
